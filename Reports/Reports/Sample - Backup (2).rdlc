  <Code>Public Function GetShamsiCurDate() As String
        Dim curdate As System.DateTime
        curdate = DateTime.Now

        Dim Tc As New System.Globalization.PersianCalendar
        Dim m As Int16
        Dim y As Int16
        Dim d As Int16
        dim ms,ds as string
        m = Tc.GetMonth(curdate)
        y = Tc.GetYear(curdate)
        d = Tc.GetDayOfMonth(curdate)
        ms=m.ToString:ds=d.ToString
        if m&lt;10 then ms="0"+ms
        if d&lt;10 then ds="0"+ds
        Return y.ToString + "/" + ms+ "/" + ds

    End Function</Code>
